{
  "version": 2,
  "builds": [
    {
      "src": "web/doctor-finder-web/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "api/server.js" },
    { "src": "/(.*)", "dest": "web/doctor-finder-web/$1" }
  ]
}